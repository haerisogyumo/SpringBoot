<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.todoappp.dao.TodoAppDAO">

	<insert id="insertTodo">
		insert into `TodoApp` values (#{todo}, #{index});
	</insert>
	
	<select id="selectTodos" resultType="kr.co.todoappp.vo.TodoAppVO">
		select * from `TodoApp`;
	</select>

	

</mapper>